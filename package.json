{
  "version": "0.0.1",
  "name": "shift-resume",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "pnpm build:remix && pnpm build:server",
    "build:remix": "remix vite:build",
    "build:server": "vite build -c vite.server.config.ts",
    "dev": "tsx --inspect ./src/express/server.ts",
    "start": "NODE_ENV=production node ./build/server.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "migration:create": "tsx node_modules/@mikro-orm/cli/esm --config mikro-orm.config.ts migration:create",
    "seed:create": "tsx node_modules/@mikro-orm/cli/esm --config mikro-orm.config.ts seeder:create",
    "db:seed": "tsx node_modules/@mikro-orm/cli/esm seeder:run --config mikro-orm.config.ts --class DevSeeder",
    "db:migrate": "tsx node_modules/@mikro-orm/cli/esm --config mikro-orm.config.ts migration:up"
  },
